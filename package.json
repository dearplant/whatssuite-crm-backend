{
  "name": "whatsapp-crm-backend",
  "version": "1.0.0",
  "description": "WhatsApp CRM SaaS Backend System",
  "main": "src/server.js",
  "type": "module",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "auto-push": "node scripts/auto-git-push.js",
    "auto-push:dry-run": "AUTO_PUSH_DRY_RUN=true node scripts/auto-git-push.js",
    "auto-push:no-tests": "AUTO_PUSH_RUN_TESTS=false node scripts/auto-git-push.js",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "swagger:generate": "node scripts/generate-swagger.js",
    "swagger:watch": "node scripts/watch-swagger.js",
    "prepare": "husky install"
  },
  "keywords": [
    "whatsapp",
    "crm",
    "saas",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.14.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/node": "^10.22.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cohere-ai": "^7.19.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "oauth-1.0a": "^2.2.6",
    "ollama": "^0.6.2",
    "openai": "^6.8.1",
    "papaparse": "^5.5.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "qrcode-terminal": "^0.12.0",
    "rate-limit-redis": "^4.2.3",
    "razorpay": "^2.9.6",
    "redis": "^5.9.0",
    "socket.io": "^4.8.1",
    "stripe": "^19.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "whatsapp-web.js": "^1.34.1",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
